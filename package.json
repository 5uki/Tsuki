{
  "name": "tsuki",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "A full-stack blogging framework built on Cloudflare + GitHub",
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "test": "pnpm -r test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css,astro}\""
  },
  "devDependencies": {
    "@tsuki/config": "workspace:*",
    "@types/node": "^22.10.5",
    "lighthouse": "^13.0.3",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.4"
}
